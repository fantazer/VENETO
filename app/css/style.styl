//.btn--mark

.section
	+below(768px)
		padding: 40px 0 0 0
	padding: 80px 0 0 0
.section-title
	+below(768px)
		font-size: 24px
		margin-bottom: 20px
	font-weight: 700
	up()
	color $mark
	font-size: 42px
	margin-bottom: 30px
	text-align center
.section-title--extend
	+below(768px)
