// slick
// === убираем выделения активности при клик ===
.slick-slide
	&:hover
		outline none!important
	&:active
		outline none!important
	&:focus
		outline none!important
.slick-slider
	position relative
// === убираем выделения активности при клике end ===

// === for custom navigation ===
.slider-wrap
	position relative

//Навигация снаружи
.slider-control
	+below(640px)
		size 30px

	flex(mid)
	tr(all)
	size 40px
	background: white
	position: absolute
	top 50%
	margin-top: -20px
	z-index 10
	cp()
	border 1px solid #efefef
	&:hover
		background: $mark
		.icon
			fill white
	.icon
		size 14px
.slider-wrap--simple
	+below(768px)
		padding: 0 10px
	.slider-control--left
		+below(768px)
			left -10px
		left -20px
	.slider-control--right
		+below(768px)
			right -10px
		right -20px

.slider-wrap--extend
	.slick-slide
		margin: 0 10px
	.slick-list
		margin: 0 -10px
	.slider-control--left
		+below(768px)
			left -40px
		left -50px
	.slider-control--right
		+below(768px)
			right -40px
		right -50px
.slick-track
	display: flex !important;
.slick-slide
	height: inherit !important;

.slick-track
	display: flex !important
	align-items:stretch

.slick-slide
	height: inherit !important
	display: flex !important
	justify-content: center
	align-items: stretch
	tr()
.slick-center
	+below(480px)
		box-shadow 0 9px 16px #a2b8c5

	transform: scale(1.2);
	box-shadow 0 18px 32px #a2b8c5
	opacity: 1 !important

//=== Внешний вид пагинации ===
.slick-dots
	+below(720px)
		bottom -20px
	width: 100%
	display: flex !important
	align-items: center
	justify-content: center
	bottom -20px
	li
		+below(720px)
			size 10px
			flex(mid)
	.dot
		+below(720px)
			flex(mid)
			size 6px
		background: $mark
		width: 20px
		height: 4px
		display: block
		br(3px)
		opacity: .2
	.slick-active
		.dot
			opacity: 1
//=== Внешний вид пагинации end===

//Навигация снаружи end

.slider-scale
	.slick-slide
		opacity: .8
	.slick-list
		+below(768px)
			padding: 80px 80px!important
		+below(640px)
			padding: 0!important
		padding: 100px 80px!important
	.slick-slide
		+below(640px)
			margin: 0
		margin: 0 20px
.slider-scale--sm
	.slick-list
		+below(480px)
			padding: 40px 20px 60px 20px!important
		padding: 60px 20px!important
// slick === end

.reestr
	+below(480px)
